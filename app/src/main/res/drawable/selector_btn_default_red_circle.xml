<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true">
        <shape >
            <solid android:color="@color/def_btn_enable_false"></solid>
            <corners android:radius="@dimen/default_btn_radius_circle"></corners>
        </shape>
    </item>
    <item android:state_enabled="false" >
        <shape>
            <solid android:color="@color/def_btn_enable_false"></solid>
            <corners android:radius="@dimen/default_btn_radius_circle"></corners>
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="@color/def_btn_red"></solid>
            <corners android:radius="@dimen/default_btn_radius_circle"></corners>
        </shape>
    </item>

</selector>